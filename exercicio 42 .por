programa {
  funcao inicio() {
    real kelvin, fahren, graus
    escreva("digite os graus para converter em fahrenheint e kelvin: ")
    leia(graus)
    fahren= (graus*9/5)+32
    kelvin= graus+273.15
    escreva("a conversao foi de ", fahren, " farenheint e de ", kelvin, " kelvin")
    //o codigo ta terminado 
  }
}